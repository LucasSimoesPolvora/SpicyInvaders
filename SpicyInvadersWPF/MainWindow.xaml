<Window x:Class="SpicyInvadersWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SpicyInvadersWPF"
        mc:Ignorable="d"
        Title="SpicyInvadersWPF" Height="1000" Width="1500">
    <Canvas Name="myCanvas" Focusable="True" Background="Black" KeyDown="KeyisDown" KeyUp="KeyisUp">
        <Label Name="Score" Foreground="White" FontSize="20" Style="{StaticResource CustomFont}" Canvas.Left="18" Canvas.Top="11" >Score :</Label>
        <Label Name="bulletLeft" Foreground="White" FontSize="20" Canvas.Left="1294" Style="{StaticResource CustomFont}" Canvas.Top="11" >Bullets Left :</Label>
        <Rectangle Name ="Player" Fill="White" Height="65" Width="56" Canvas.Left="722" Canvas.Top="761" HorizontalAlignment="Center" VerticalAlignment="Top"></Rectangle>
    </Canvas>
</Window>
